{
    "builds": [
        {
            "src": "api/main.py",
            "use": "@vercel/python",
            "config": { "pip": "api/requirements.txt" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/main.py"
        }
    ],
  "functions": {
    "api/index.py": {
      "includeFiles": "models/**"
    }
  }
}

